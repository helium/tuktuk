    "prebuild": "npm run clean && npm run package"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/web3.js": "^1.95.2"
  },
  "devDependencies": {
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "gitHead": "5a8bf0b7b88e5934ef8d774e686f7c95804fbb8d"
