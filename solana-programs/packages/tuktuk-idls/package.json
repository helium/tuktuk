{
  "name": "@helium/tuktuk-idls",
  "version": "0.0.4",
  "description": "Exported idls",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/helium/tuktuk"
  },
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "exports": {
    "./*": "./*"
  },
  "scripts": {
