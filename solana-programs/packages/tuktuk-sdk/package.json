{
  "name": "@helium/tuktuk-sdk",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "Apache-2.0",
  "version": "0.0.8",
  "description": "Interface to the tuktuk smart contract",
  "repository": {
    "type": "git",
    "url": "https://github.com/helium/tuktuk"
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/src/index.js",
  "types": "./lib/types/src/index.d.ts",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "exports": {
    "import": "./lib/esm/src/index.js",
    "require": "./lib/cjs/index.js",
    "types": "./lib/types/src/index.d.ts"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "precommit": "npx git-format-staged -f 'prettier --ignore-unknown --stdin --stdin-filepath \"{}\"' .",
    "clean": "mkdir -p lib && rm -rf lib",
    "package": "mkdir -p lib/cjs lib/esm",
    "prebuild": "npm run clean && npm run package"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.0",
    "@helium/anchor-resolvers": "^0.10.0-alpha.4",
    "@helium/tuktuk-idls": "^0.0.8",
    "js-sha256": "^0.11.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "git-format-staged": "^2.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "gitHead": "5a8bf0b7b88e5934ef8d774e686f7c95804fbb8d"
}
